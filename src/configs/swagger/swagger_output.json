{
  "swagger": "2.0",
  "info": {
    "title": "Cashback API",
    "version": "1.0.0",
    "description": ""
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [],
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "apiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization"
    }
  },
  "consumes": [],
  "produces": [],
  "paths": {
    "/api/autenticacao": {
      "post": {
        "tags": [
          "Autenticacao"
        ],
        "description": "Fazer login",
        "parameters": [
          {
            "name": "Dados do revendedor",
            "in": "body",
            "type": "object",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "example": "email@email.com"
                },
                "senha": {
                  "type": "string",
                  "example": "senha_segura"
                }
              },
              "required": [
                "email",
                "senha"
              ]
            }
          }
        ],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/revendedores": {
      "post": {
        "tags": [
          "Revendedores"
        ],
        "description": "Registrar um revendedor",
        "parameters": [
          {
            "name": "Dados do revendedor",
            "in": "body",
            "type": "object",
            "schema": {
              "type": "object",
              "properties": {
                "nome": {
                  "type": "string",
                  "example": "Jorge"
                },
                "cpf": {
                  "type": "string",
                  "example": "12345678900"
                },
                "email": {
                  "type": "string",
                  "example": "email@email.com"
                },
                "senha": {
                  "type": "string",
                  "example": "senha_segura"
                }
              },
              "required": [
                "nome",
                "cpf",
                "email",
                "senha"
              ]
            }
          }
        ],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/compras": {
      "post": {
        "tags": [
          "Compras"
        ],
        "description": "Registrar uma compra",
        "parameters": [
          {
            "name": "Dados da compra",
            "in": "body",
            "type": "object",
            "schema": {
              "type": "object",
              "properties": {
                "codigo": {
                  "type": "string",
                  "example": "abc"
                },
                "valor": {
                  "type": "number",
                  "example": 10
                },
                "cpf": {
                  "type": "string",
                  "example": "12345678900"
                },
                "data": {
                  "type": "string",
                  "example": "2021/08/19"
                }
              },
              "required": [
                "codigo",
                "valor",
                "cpf",
                "data"
              ]
            }
          }
        ],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      },
      "get": {
        "tags": [
          "Compras"
        ],
        "description": "Listar compras",
        "parameters": [
          {
            "name": "mes",
            "in": "query",
            "type": "string",
            "description": "exemplo: 8"
          }
        ],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      }
    },
    "/api/cashback-acumulado": {
      "get": {
        "tags": [
          "Compras"
        ],
        "description": "Exibir acumulado de cashback",
        "parameters": [],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "security": [
          {
            "apiKeyAuth": []
          }
        ]
      }
    }
  },
  "definitions": {}
}