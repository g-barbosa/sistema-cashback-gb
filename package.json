{
 "name": "tcc",
 "version": "1.0.0",
 "main": "index.js",
 "license": "MIT",
 "scripts": {
  "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/server.ts",
  "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
  "knex:seed": "knex --knexfile knexfile.ts seed:run",
  "swagger-autogen": "node swagger.js",
  "test": "jest --passWithNoTests --noStackTrace --runInBand",
  "test:cov": "yarn test --coverage"
 },
 "author": "Giovanne Brito Barbosa",
 "dependencies": {
  "axios": "^0.21.1",
  "bcrypt": "^5.0.0",
  "body-parser": "^1.19.0",
  "cors": "^2.8.5",
  "dotenv": "^8.2.0",
  "ejs": "^3.1.5",
  "express": "^4.17.1",
  "express-session": "^1.17.1",
  "jsonwebtoken": "^8.5.1",
  "knex": "^0.21.5",
  "mysql": "^2.18.1",
  "swagger-autogen": "^2.11.2",
  "swagger-ui-express": "^4.1.6",
  "ts-jest": "^27.0.5",
  "uuid-random": "^1.3.2",
  "winston": "^3.3.3"
 },
 "devDependencies": {
  "@types/bcrypt": "^3.0.0",
  "@types/cors": "^2.8.7",
  "@types/express": "^4.17.7",
  "@types/express-session": "^1.17.0",
  "@types/jest": "^27.0.1",
  "@types/jsonwebtoken": "^8.5.5",
  "@types/knex": "^0.16.1",
  "@types/swagger-ui-express": "^4.1.3",
  "jest": "^27.0.6",
  "supertest": "^6.1.6",
  "ts-node-dev": "^1.0.0-pre.61",
  "typescript": "^4.0.2"
 },
 "jest": {
  "moduleFileExtensions": [
   "js",
   "json",
   "ts"
  ],
  "rootDir": ".",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
   "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
   "<rootDir>/src/services/*.ts",
   "!**/index.ts"
  ],
  "coverageDirectory": "coverage",
  "testEnvironment": "node"
 }
}
